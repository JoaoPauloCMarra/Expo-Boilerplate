{
  "name": "expo-boilerplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:c": "watchman watch-del-all & watchman shutdown-server && expo start -c",
    "open:ios": "open -a \"Xcode\" ios",
    "open:android": "open -a \"Android Studio\" android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "web:build": "npx expo export -p web",
    "web:deploy": "netlify deploy --dir dist",
    "web:deploy:prod": "netlify deploy --prod --dir dist",
    "native:build:all": "eas build --platform all",
    "native:build:ios": "eas build --platform ios",
    "native:build:android": "eas build --platform android",
    "pod": "bunx pod-install@latest",
    "test:unit": "jest --maxWorkers=50%",
    "test:unit:watch": "jest --watch --maxWorkers=25%",
    "test:unit:ci": "jest --runInBand",
    "expo:fix": "expo install --fix",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20",
    "bun": ">=1"
  },
  "dependencies": {
    "@expo/html-elements": "^0.10.1",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@expo/vector-icons": "^14.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-community/slider": "4.5.2",
    "@react-navigation/native": "^6.1.17",
    "@tanstack/react-query": "^5.35.5",
    "clsx": "^2.1.1",
    "expo": "^51.0.2",
    "expo-build-properties": "~0.12.1",
    "expo-camera": "~15.0.5",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "~4.0.13",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.4",
    "expo-haptics": "~13.0.1",
    "expo-image-picker": "~15.0.4",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-router": "~3.5.11",
    "expo-secure-store": "~13.0.1",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "jotai": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.4",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.11",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-export-namespace-from": "^7.24.1",
    "@expo/webpack-config": "~19.0.1",
    "@tanstack/eslint-plugin-query": "^5.32.1",
    "@testing-library/react-native": "^12.5.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/typescript-estree": "^7.8.0",
    "ajv": "^8.13.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hook-form": "^0.3.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  }
}
